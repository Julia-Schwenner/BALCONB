
<div class="page-container">
<h1>Prepare yourself to have a paw-some moment.</h1>
<h3>Here are your upcoming meetings</h3>

<% @meetings.each do |meeting| %>
  <table>
    <thead>
      <th colspan="3">
      <tr>
        <th>📅 </th>
        <td><%= meeting.dates %></td>
        <th> 🐕</th>
        <td><%= meeting.dog.name %></td>
        <th>💲 </th>
        <td><%= meeting.price %></td>
        <td><%= link_to '🗑', meeting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      </th>
    </thead>
  </table>
<% end %>

<br>


</div>

