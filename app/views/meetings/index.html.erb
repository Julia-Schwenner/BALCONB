<div class="page-container">
<h3>Prepare to have a pawsome moment</h3>
<h5>Your date</h5>


<% @meetings.each do |meeting| %>
  <table>
    <thead>
      <th colspan="3">
      <tr>
        <th>📅 </th>
        <td><%= meeting.dates %></td>
        <th> 🐕</th>
        <td><%= meeting.dog.name %></td>
        <th>💲 </th>
        <td><%= meeting.price %></td>
        <td><%= link_to '🗑', meeting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      </th>
    </thead>
  </table>
<% end %>

<%= link_to 'Find a new friend!', dogs_path %>
<br>

</div>

